{
  "name": "tableside-users",
  "version": "0.1.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn prisma:gen && tsc --project ./",
    "prisma:gen": "prisma generate",
    "prisma:migrate:create": "prisma migrate dev --create-only",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "start": "node dist/server.js",
    "start:migrate": "yarn prisma:migrate:deploy && yarn start",
    "start:dev": "nodemon src/server.ts",
    "start:migrate:dev": "yarn prisma:migrate:create && yarn start:dev"
  },
  "dependencies": {
    "@prisma/client": "5.9.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "nodemon": "^3.0.3",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
